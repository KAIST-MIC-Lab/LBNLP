@Article{2016_Abdelrahem_Simplifiedmodelpredictivecurrentcontrolwithoutmechanicalsensorsforvariable-speedwindenergyconversionsystems,
  author    = {Abdelrahem, Mohamed and Hackl, Christoph Michael and Kennel, Ralph},
  title     = {Simplified model predictive current control without mechanical sensors for variable-speed wind energy conversion systems},
  doi       = {10.1007/s00202-016-0433-y},
  issn      = {1432-0487},
  number    = {1},
  pages     = {367--377},
  url       = {http://dx.doi.org/10.1007/s00202-016-0433-y},
  volume    = {99},
  abstract  = {This paper proposes a simplified finite-control-set model predictive current control (FCS-MPCC) without mechanical sensors for permanent-magnet synchronous generators (PMSGs) in variable-speed wind energy conversion systems. The procedure of selecting the best switching vector is optimized by computing the reference voltage vector (VV) directly from the reference current. Subsequently, the sector where this reference VV is located is determined from its angle. Finally, the cost function is evaluated only for three times to obtain the optimal switching vector. Therefore, the necessity to test all feasible VVs will be avoided, which reduces the calculation burden of the traditional finite-control-set model predictive control method. Moreover, an extended Kalman filter, which is a robust state observer, is proposed to estimate rotor speed, rotor position, and stator inductance of the PMSG. The estimated (filtered) stator currents, instead of the measured currents, are fed back to the prediction model, and therefore, a lower current total harmonic distortion and better noise rejection are realized. Estimation and control performance of the proposed simplified FCS-MPCC method are illustrated by the simulation results for all operation conditions.},
  file      = {:2016_Abdelrahem_Simplifiedmodelpredictivecurrentcontrolwithoutmechanicalsensorsforvariable-speedwindenergyconversionsystems.pdf:PDF},
  journal   = {Electrical Engineering},
  owner     = {chackl},
  publisher = {Springer Nature},
  timestamp = {2016.09.20},
  year      = {2016},
	creationdate = {2016-09-20T00:00:00},
}

