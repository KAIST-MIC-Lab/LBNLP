  keywords   = {Generators, Insulated gate bipolar transistors, Power harmonic filters, Power transformers, Reactive power, Solid state circuits, Voltage control},
  month      = jun,
  owner      = {chackl},
  shorttitle = {The {Smart} {Transformer}},
