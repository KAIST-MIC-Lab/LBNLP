@Article{2007_Roshen_IronLossModelforPMSMs,
  author    = {Roshen, W.},
  title     = {Iron {Loss} {Model} for {Permanent}-{Magnet} {Synchronous} {Motors}},
  doi       = {10.1109/TMAG.2007.899687},
  issn      = {0018-9464},
  number    = {8},
  pages     = {3428--3434},
  volume    = {43},
  abstract  = {Iron losses in permanent-magnet synchronous machines form a larger portion of the total losses than in induction machines and, hence, more importance should be given to the iron losses. Previously, models have been presented for the calculations of these losses, but these models still rely on finite-element simulations to obtain correction factors, which are substantial, to apply to the theoretically derived formulas in order to obtain good agreement with the experimental data. This paper points out the source of this correction factor: the neglect of the excess eddy-current loss component. In many cases, this loss component dominates the total iron losses and needs to be incorporated in the theoretical considerations. The paper also provides a more complete model of iron loss, which greatly reduces the need for calculating the correction factors using the finite-element method (FEM). This more complete model reduces design time, especially when a number of candidate designs need to be analyzed. Otherwise, the calculation of the correction factors using FEM would be cumbersome, as the correction factors tend to be nonlinear.},
  file      = {:2007_Roshen_IronLossModelforPMSMs.pdf:PDF},
  journal   = {{IEEE} Transactions on Magnetics},
  keywords  = {AC machines, AC motors, correction factors, eddy-current loss component, eddy current losses, eddy-current losses, finite element analysis, finite-element method, Finite element methods, Hysteresis, induction machines, Iron, Iron alloys, iron loss model, losses, Magnetic analysis, magnetic flux, Magnetic losses, permanent magnet motors, permanent-magnet synchronous motors, synchronous machines, synchronous motors},
